{
    "scripts": {
        "lint": "NODE_PATH=src eslint --ignore-path .gitignore .",
        "jest:test": "jest",
        "start": "npm run dev",
        "prod:build-server": "API_DOMAIN=webapi.wral.com NODE_ENV=production webpack --config webpack.server.js",
        "prod:build-client": "API_DOMAIN=webapi.wral.com NODE_ENV=production webpack -p --config webpack.client.js --mode production",
        "dev": "npm run build-server && npm-run-all --parallel dev:*",
        "build-server": "NODE_ENV=development webpack --config webpack.server.js",
        "dev:build-client": "NODE_ENV=development webpack --config webpack.client.js --verbose --watch",
        "dev:server": "NODE_ENV=development nodemon --watch build --exec \"node build/bundle.js\"",
        "precommit": "npm run lint",
        "new:prod:build": "rm -rf build/* && rm -rf public/* && NODE_ENV=production webpack -p --mode production",
        "new:dev": "NODE_ENV=development rm -rf public/* && npm run new:build-server && npm-run-all --parallel dev:server",
        "new:build-server": " NODE_ENV=development webpack --config webpack.config.js -d --progress --verbose",
        "build:webpack": "webpack"
    },
